#:kivy 1.9.0
#:import ttf gui.font.ttf

<ScrollView>:
	size_hint_x: None
	width: 500
	scroll_type: ['bars']
	do_scroll: True, True
	bar_width: 5

<FileManager>:
	cols: 1
	size_hint: .6, .9
	pos_hint: {'x': .05, 'top': .95}
	canvas:
		Color:
			rgba: 1, 1, 1, 1
		Rectangle:
			pos: self.pos
			size: self.size
